module Frontend.Signe.Skel where

-- Haskell module generated by the BNF converter

import Frontend.Signe.Abs
import Frontend.Signe.ErrM
type Result = Err String

failure :: Show a => a -> Result
failure x = Bad $ "Undefined case: " ++ show x

transId :: Id -> Result
transId x = case x of
  Id string -> failure x
transScalar :: Scalar -> Result
transScalar x = case x of
  Scalar string -> failure x
transProgram :: Program -> Result
transProgram x = case x of
  Progr toplevels -> failure x
transToplevel :: Toplevel -> Result
transToplevel x = case x of
  ToplF id patterns expr -> failure x
  ToplFT id patterns type_ expr -> failure x
transExpr :: Expr -> Result
transExpr x = case x of
  EVar id -> failure x
  ETrue -> failure x
  EFalse -> failure x
  ETup exprs -> failure x
  EApp expr1 expr2 -> failure x
  ESup complex1 expr1 supop complex2 expr2 -> failure x
  EMul complex expr -> failure x
  EComp expr1 expr2 -> failure x
  EIfq expr1 expr2 expr3 -> failure x
  EIf expr1 expr2 expr3 -> failure x
  ELet lets expr -> failure x
  EAbs ids expr -> failure x
transLet :: Let -> Result
transLet x = case x of
  LLet pattern expr -> failure x
transSupOp :: SupOp -> Result
transSupOp x = case x of
  SOPlus -> failure x
  SOMin -> failure x
transComplex :: Complex -> Result
transComplex x = case x of
  CComp scalar1 scalar2 -> failure x
  CComn scalar1 scalar2 -> failure x
  CPi -> failure x
  CE -> failure x
  CExp complex1 complex2 -> failure x
  CDiv complex1 complex2 -> failure x
  CMul complex1 complex2 -> failure x
transPattern :: Pattern -> Result
transPattern x = case x of
  Pattern patternvar -> failure x
  Patterns patternvars -> failure x
transPatternVar :: PatternVar -> Result
transPatternVar x = case x of
  PVar id -> failure x
transMono :: Mono -> Result
transMono x = case x of
  MVar id -> failure x
  MQubit -> failure x
  MUnit -> failure x
  MTens mono1 mono2 -> failure x
  MArrow mono1 mono2 -> failure x
transType :: Type -> Result
transType x = case x of
  TMono mono -> failure x
  TPoly ids mono -> failure x

